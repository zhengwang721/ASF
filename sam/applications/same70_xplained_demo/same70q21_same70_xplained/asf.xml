<asf xmlversion="1.0">
	<project caption="WINC1500 Weather Client Demo for SAME70 Xplained" id="sam.applications.same70_xplained_demo.same70_xplained">
		<info type="description" value="summary">
		This demonstration program retrieve and display temperature and weather of several cities from openweathermap.org server by WINC1500 WIFI module.
		</info>
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="header-file" value="conf_winc.h"/>
		<build type="header-file" value="conf_sleepmgr.h"/>
		<build type="header-file" value="conf_ili9488.h"/>
		<build type="header-file" value="conf_spi_master.h"/>
		<build type="include-path" value="."/>
		<require idref="sam.applications.same70_xplained_demo"/>
		<require idref="board.same70_xplained"/>
		<config name="config.sam.components.display.ili9488" value="spi"/>
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
		<device-support value="atsame70q21"/>
	</project>
</asf>
